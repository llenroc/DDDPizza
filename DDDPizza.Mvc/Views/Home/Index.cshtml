@model List<DDDPizza.ViewModels.OrderVm>
@{
    ViewBag.Title = "Back Office Pizza!";
}

<div class="jumbotron">
    <h2>Current Orders</h2>
</div>

<div class="row">
    <div class="col-md-12">
        
        <table class="table table-bordered table-responsive table-striped">
            <tr>
                <td></td>
                <td></td>
                <td></td>
                <td></td>
                <td></td>
            </tr>

            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.Id</td>
                    <td>@item.SubTotal</td>
                    <td>@item.ServiceCharge</td>
                    <td>@item.TotalAmount</td>
                    <td>@item.ServiceType</td>
                </tr>
            }
        </table>     

    </div>
</div>